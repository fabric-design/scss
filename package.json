  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fabric-design/scss.git"
  },
  "scripts": {
    "lint": "gulp lint-sass",
    "prestart": "npm install",
    "test": "gulp"
  },
  "stylelint": {
    "extends": "stylelint-config-sass-guidelines",
    "plugins": [
      "stylelint-scss"
    ],
    "rules": {
      "max-nesting-depth": 2,
      "order/properties-alphabetical-order": null,
      "selector-no-qualifying-type": [
        true,
        {
          "ignore": [
            "attribute",
            "class"
          ]
        }
      ]
    }
  }
